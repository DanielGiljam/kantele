{
    "name": "project-lobe",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/project-lobe/src",
    "targets": {
        "build": {
            "executor": "@nx-plus/docusaurus:browser",
            "options": {
                "outputPath": "dist/apps/project-lobe"
            }
        },
        "serve": {
            "executor": "@nx-plus/docusaurus:dev-server",
            "options": {
                "port": 3000
            }
        },
        "type-check": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "command": "tsc --build apps/project-lobe/tsconfig.json --incremental"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/project-lobe/**/*.{ts,tsx,js,jsx}"]
            }
        }
    },
    "tags": []
}
